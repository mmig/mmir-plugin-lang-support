{
  "name": "mmir-plugin-lang-support",
  "version": "1.1.0",
  "description": "tools for querying supported languages (ASR and TTS) and voices (TTS) by Nuance SpeechKit",
  "main": "./index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "./www"
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-metadata",
    "build-ts": "tsc",
    "build-metadata": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-plugin-lang-support.git"
  },
  "keywords": [
    "speech",
    "asr",
    "tts",
    "language"
  ],
  "author": "russa",
  "license": "MIT",
  "devDependencies": {
    "mmir-lib": "^5.2.0",
    "mmir-plugin-exports": "^1.2.1",
    "typescript": "^3.6.3"
  }
}
